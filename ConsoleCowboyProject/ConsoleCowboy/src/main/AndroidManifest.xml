<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.velcal.consolecowboy"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.velcal.consolecowboy.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.velcal.consolecowboy.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.velcal.consolecowboy.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.velcal.consolecowboy.MainActivity"/>
        </activity>
        <activity
            android:name="com.velcal.consolecowboy.BoardActivity"
            android:label="@string/title_activity_board"
            android:parentActivityName="com.velcal.consolecowboy.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.velcal.consolecowboy.MainActivity"/>
        </activity>
    </application>

</manifest>
